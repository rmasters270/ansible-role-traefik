apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: default-certificate
  namespace: default
spec:
  secretName: default-certificate
  issuerRef:
    name: {{ traefik_default_cert_issuer }}
    kind: {{ traefik_default_cert_issuer_kind }}
  dnsNames:
    - "{{ traefik_default_cert_dnsname }}"
